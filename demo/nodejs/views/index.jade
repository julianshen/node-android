extends layout

block content
  h1= title
  p Welcome to #{title}
  script(src='/socket.io/socket.io.js')
  script.
    var socket = io.connect('http://localhost:3000');
    socket.on('news', function (data) {
        console.log(data);
        document.write(data.hello);
        socket.emit('my other event', {my: 'data'}); 
    });
